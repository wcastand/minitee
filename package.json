{
  "name": "minitee",
  "version": "0.0.0-semantic-dummy",
  "description": "mini template engine",
  "main": "./lib/index.js",
  "bin": {
    "minitee": "./lib/index.js"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel --copy-files src/ -d lib/",
    "watch": "babel -w src/ -d lib/",
    "commit": "git-cz",
    "test": "mocha test/index.test.js --compilers js:babel-register",
    "test:single": "mocha test/index.test.js --compilers js:babel-register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "template",
    "mini"
  ],
  "author": "William Castandet <wcastand@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wcastand/minitee.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "commitizen": "^2.8.1",
    "cz-conventional-changelog": "^1.1.6",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "front-matter": "^2.0.7",
    "mkdirp": "^0.5.1",
    "ramda": "^0.21.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
